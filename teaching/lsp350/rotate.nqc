// Using the rotation sensor with tribot.nqc//	three wheeled robot (from Guide to NQC)// motors and sensors#define LEFT OUT_A#define RIGHT OUT_C#define BUMP SENSOR_1#define ANGLE SENSOR_3// constants#define REV_TIME	50#define TURN_TIME	20task main(){	//Set up sensors	SetSensor(BUMP, SENSOR_TOUCH);	SetSensor(ANGLE, SENSOR_ROTATION);		Fwd(LEFT);	Rev(RIGHT);	On(LEFT+RIGHT);		//Turn exactly 360 degrees	until(ANGLE == 16);	Off(LEFT+RIGHT);	PlaySound(SOUND_UP);	}