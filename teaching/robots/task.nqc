// task.nqc //	an example of multiple tasks//	note that tasks that haven't been stopped, continue//	after the main task ends.// UCD COSMOS, Cluster V, Summer 2001task main(){    //Start turning in place:    start turn_in_place;   //Start beeping slowly    start slow_beep;    Wait(600);    start slow_beep;        //Stop beeping after 4 seconds    //Wait(400);    //stop slow_beep;}task turn_in_place(){    while (true)    {	Fwd(OUT_A);	Rev(OUT_C);	On(OUT_A+OUT_C);    }}task slow_beep(){    int i,j;        StopAllTasks();         for (i = 0; i < 20; i++)    {		    	for (j= 0 ; j < i+1; j++)    	{    		PlayTone(880,10);    		Wait(25);    	}		Wait(100);    }}