// task.nqc //	an example of multiple tasks//	note that tasks that haven't been stopped, continue//	after the main task ends.// UCD COSMOS, Cluster V, Summer 2001task main(){    //Start turning in place:    start turn_in_place;    //Start beeping slowly    start slow_beep;    //Stop beeping after 4 seconds    Wait(400);    stop slow_beep;        Wait(400);    stop turn_in_place;    Off(OUT_A+OUT_C);}task turn_in_place(){    while (true)    {	Fwd(OUT_A);	Rev(OUT_C);	On(OUT_A+OUT_C);    }}task slow_beep(){    while (true)    {	PlayTone(880,10);	Wait(100);    }}