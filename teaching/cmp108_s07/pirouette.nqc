// pirouette.nqc - turn 360 degrees and beep!// UCD COSMOS, Cluster V, Summer 2001#define LEFT OUT_A#define RIGHT OUT_C//Time to turn 360 degrees#define TURN_TIME 230task main(){int i;i=0;  for( i = 0; i < 4 ; i++)  {	Fwd(LEFT+RIGHT);	On(LEFT+RIGHT);	Wait(200);	    //Turn in place, to the right    Fwd(LEFT);    Rev(RIGHT);    On(LEFT+RIGHT);    //Stop after a full revolution and beep!    Wait(TURN_TIME/4);    Off(LEFT+RIGHT);    PlaySound(SOUND_UP);  }}